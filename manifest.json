{
  "name": "Standardized Screenshot",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "Takes standardized screenshots.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "activeTab" ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
