{
	"manifest_version": 2,
	"name": "Capture Page State",
	"description": "",
	"version": "0.0.1",
	"author": "Salvador Hernandez",
	"devtools_page": "devtools.html",
	"web_accessible_resources": [
		"/images/checkMark.png"
	],
	"background": {
		"scripts": ["runOnLoad.js"],
		"persistent": true
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"storage",
		"downloads",
		"http://*/*",
		"https://*/*",
		"background",
		"tabs"
	]
}