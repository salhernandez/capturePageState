{
	"manifest_version": 2,
	"name": "Capture Page State",
	"description": "",
	"version": "0.0.1",
	"author": "Salvador Hernandez",
	"devtools_page": "devtools.html",
	"web_accessible_resources": [
		"/images/checkMark.png"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": true
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Capture Page State"
	},
	"permissions": [
		"activeTab",
		"storage",
		"downloads",
		"notifications",
		"activeTab",
		"<all_urls>",
		"http://*/*",
		"https://*/*",
		"background",
		"tabs",
		"debugger"
	]
}